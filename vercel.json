{
  "version": 2,
  "builds": [
    {
      "src": "src/public/**",
      "use": "@now/static"
    },
    {
      "src": "src/index.js",
      "use": "@now/node@canary"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/MyProjects",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/Blog",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/Blog/(?<page>[^/]*)",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/Blog/Post/(?<id>[^/]*)",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/Contact",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/Contact",
      "dest": "src/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/AdminSignIn",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/AdminSignIn",
      "dest": "src/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/AdminMyProfile",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/AdminManageProjects",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/AdminManageProjects",
      "dest": "src/index.js",
      "methods": ["DELETE"]
    },
    {
      "src": "/AdminEditProject",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/AdminEditProject",
      "dest": "src/index.js",
      "methods": ["PUT"]
    },
    {
      "src": "/AdminAddProject",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/AdminAddProject",
      "dest": "src/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/AdminAddPost",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/AdminAddPost",
      "dest": "src/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/Cookies",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/PrivacyPolicy",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/UserManual",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/LegalNotice",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/Project/(?<id>[^/]*)",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/SignOut",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/dbTest",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "src/public/$1"
    }
  ]
}
