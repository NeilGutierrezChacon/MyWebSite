{
    "source":{
        "include":["src"],
        "includePattern":".js$",
        "excludePattern":"(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks":true,
        "monospaceLinks":true
    },
    "opts":{
        "recurse":true,
        "destination":"./src/public/docs/",
        "readme":"./JsDoc.md",
        "template":"./ngch43-template-jsDoc"
    }
}